                            "duration": 0.1
                        }
                    ]
                }
            },
            "c:crash": {
                "minecraft:scale": {
                    "max": 0,
                    "value": 0
                },
                "minecraft:despawn": {
                    "filters": {
                        "any_of": [
                            {
                                "test": "is_family",
                                "subject": "self",
                                "value": "player"
                            }
                        ]
                    }
                },
                "minecraft:spell_effects": {
                    "add_effects": [
                        {
                            "effect": "invisibility",
                            "amplifier": 1,
                            "visible": false,
                            "ambient": false,
                            "duration": 0.1
                        }
                    ]
                }
            }
        },
        "components": {
            "minecraft:type_family": {
                "family": [
                    "player"
                ]
            },
            "minecraft:loot": {
                "table": "loot_tables/empty.json"
            },
            "minecraft:collision_box": {
                "width": 0.6,
                "height": 1.8
            },
            "minecraft:can_climb": {},
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:environment_sensor": [
                {
                    "on_environment": {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "self",
                                    "value": "server_standard"
                                }
                            ]
                        },
                        "event": "server:reset_player"
                    }
                },
                {
                    "on_environment": {
                        "filters": {
