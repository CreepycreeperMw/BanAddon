{
    "format_version": "1.8.0",
    "minecraft:entity": {
        "description": {
            "identifier": "minecraft:player",
            "is_spawnable": false,
            "is_summonable": false,
            "is_experimental": false
        },
        "component_groups": {
            "v:vanish": {
                "minecraft:scale": {
                    "max": 0,
                    "value": 0
                },
                "minecraft:spell_effects": {
                    "add_effects": [
                        {
                            "effect": "invisibility",
                            "amplifier": 1,
