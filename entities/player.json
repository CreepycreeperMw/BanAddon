                        "min_rider_count": 1,
                        "max_rider_count": 2,
                        "lock_rider_rotation": 0
                    }
                ]
            },
            "minecraft:scaffolding_climber": {}
        },
        "events": {
            "c:crash": {
                "add": {
                    "component_groups": [
                        "c:crash"
                    ]
                }
            },
            "c:unban": {
                "remove": {
                    "component_groups": [
                        "c:unban"
                    ]
                }
            },
            "v:has_vanish": {
                "add": {
                    "component_groups": [
                        "v:vanish"
                    ]
                }
            },
            "v:has_unvanish": {
