                        },
                        "event": "c:crash"
                    }
                },
                {
                    "on_environment": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "subject": "self",
                                    "value": "vanish"
                                }
                            ]
                        },
                        "event": "v:has_vanish"
                    }
                },
                {
                    "on_environment": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "subject": "self",
                                    "value": "unvanish"
                                }
                            ]
                        },
                        "event": "v:has_unvanish"
                    }
                },
                {
                    "on_environment": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_tag",
                                    "subject": "self",
                                    "value": "unban"
                                }
                            ]
                        },
                        "event": "c:unban"
                    }
                }
            ],
            "minecraft:player.saturation": {
                "value": 20
            },
            "minecraft:player.exhaustion": {
                "value": 0,
                "max": 4
